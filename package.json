{
  "name": "transfer-learning-competition",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "xo"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.4.1",
    "@mui/material": "^5.4.0",
    "@prisma/client": "4",
    "@types/bcrypt": "^5.0.0",
    "@types/busboy": "^1.3.0",
    "@types/nodemailer": "^6.4.4",
    "axios": "^0.25.0",
    "bcrypt": "^5.0.1",
    "busboy": "^1.4.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "eslint": "^8.8.0",
    "formik": "^2.2.9",
    "iron-session": "^6.0.5",
    "next": "12.0.10",
    "nodemailer": "^6.7.2",
    "prisma": "4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sharp": "^0.30.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.10",
    "@types/node": "17.0.15",
    "@types/react": "17.0.39",
    "eslint-config-xo-react": "^0.26.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "typescript": "4.5.5",
    "xo": "^0.47.0"
  },
  "xo": {
    "extends": [
      "xo-react",
      "plugin:@next/next/recommended"
    ],
    "prettier": true,
    "rules": {
      "react/require-default-props": "off",
      "react/prop-types": "off",
      "unicorn/filename-case": [
        "error",
        {
          "cases": {
            "camelCase": true,
            "pascalCase": true,
            "kebabCase": true
          },
          "ignore": [
            ".*\\.config\\.(js|ts)$"
          ]
        }
      ],
      "@typescript-eslint/naming-convention": [
        "warn",
        {
          "selector": "function",
          "format": [
            "camelCase",
            "PascalCase"
          ]
        }
      ],
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "ts": "never",
          "tsx": "never"
        }
      ]
    },
    "ignores": [
      "next-env.d.ts",
      "next.config.js",
      "lint-staged.config.js",
      ".next/",
      "node_modules/"
    ]
  }
}
